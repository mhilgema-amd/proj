#%Module -*- tcl -*-
##
## dot modulefile
##
set             module_prefix       /opt
set             vendor              amd
set             app                 minihpl
set             version             0.1.0
set             root                $module_prefix/$vendor/$app/$version

prepend-path    PATH                $root

proc ModulesHelp { } {
    puts stderr "\tAdds $app $version to your environment variables,"
}
